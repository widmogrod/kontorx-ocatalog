if (null != $this->paginator) {
	/* @var $paginationControl Zend_View_Helper_PaginationControl */
	$paginationControl = $this->getHelper('PaginationControl');
	
	print $paginationControl->paginationControl($this->paginator,
                             'Sliding',
                             '_partial/newPagination.phtml');
}
?>